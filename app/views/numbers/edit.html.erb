<%= turbo_frame_tag dom_id(@number) do %>
	<%= form_with url: profile_number_path(@profile_id, @number.id), method: :patch do |f| %>
		<div class="lined-input">
			<li>
				<%= f.text_field "number", value: @number.number, placeholder: "Number", autocomplete: "off", required: true %>
				<%= f.button class: "ui compact icon button" do %>
					<i class="save blue icon"></i>
				<% end %>
				<%= link_to new_profile_number_path do %>
					<i class="close blue link icon"></i>
				<% end %>
			</li>
		</div>
	<% end %>
<% end %>