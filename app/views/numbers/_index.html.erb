<% @user.numbers.each do |number| %>
	<%= turbo_frame_tag dom_id(number) do %>
		<li>
			<div class="inline-edit"><%= number.number %></div>
			<%= link_to edit_profile_number_path(@user.id, number.id) do %>
				<i class="edit outline blue link icon"></i>
			<% end %>
			<%= link_to "/profiles/#{@user.id}/numbers/#{number.id}", data: {turbo_method: :delete} do %>
				<i class="trash alternate outline blue link icon"></i>
			<% end %>
		</li>
	<% end %>
<% end %>
